-- -------------------------
-- Table for Announcements / Events
-- -------------------------
CREATE TABLE IF NOT EXISTS events (
    event_id INTEGER PRIMARY KEY AUTOINCREMENT,
    event_title TEXT NOT NULL,
    event_description TEXT,
    event_date TEXT NOT NULL,
    program_id INTEGER,
    FOREIGN KEY (program_id) REFERENCES programs(program_id)
);

-- Sample Announcements
INSERT INTO events (event_title, event_description, event_date, program_id) VALUES
('Alumni Homecoming 2025', 'Join us for food, music, and fellowship with fellow graduates.', '2025-03-21', 1),
('Job Fair 2025', 'Meet top IT industry employers and submit your resumes.', '2025-04-10', 2),
('TechTalk Seminar', 'A seminar about new emerging technologies in the industry.', '2025-02-18', 3);

-- -------------------------
-- Table for Careers
-- -------------------------
CREATE TABLE IF NOT EXISTS careers (
    career_id INTEGER PRIMARY KEY AUTOINCREMENT,
    alumni_id INTEGER,
    company_name TEXT NOT NULL,
    position TEXT NOT NULL,
    start_date TEXT NOT NULL,
    end_date TEXT,
    FOREIGN KEY (alumni_id) REFERENCES alumni(alumni_id)
);

-- Sample Careers
INSERT INTO careers (alumni_id, company_name, position, start_date, end_date) VALUES
(1, 'TechCorp', 'Software Engineer', '2025-06-01', NULL),
(2, 'CyberSoft', 'Network Administrator', '2024-01-15', NULL),
(3, 'Innovatech', 'Data Analyst', '2025-05-20', NULL);

-- -------------------------
-- Table for Admin
-- -------------------------
CREATE TABLE IF NOT EXISTS admin (
    admin_id INTEGER PRIMARY KEY AUTOINCREMENT,
    username TEXT UNIQUE NOT NULL,
    password TEXT NOT NULL
);

-- Sample Admin
INSERT INTO admin (username, password) VALUES
('admin', 'admin123');  -- In production, hash passwords!

-- Add password column to alumni
ALTER TABLE alumni ADD COLUMN password TEXT;

