-- Drop existing tables (optional, if you want to start fresh)
-- -------------------------
DROP TABLE IF EXISTS careers;
DROP TABLE IF EXISTS events;
DROP TABLE IF EXISTS alumni;
DROP TABLE IF EXISTS programs;
DROP TABLE IF EXISTS admin;

-- -------------------------
-- Table for Programs
-- -------------------------
CREATE TABLE IF NOT EXISTS programs (
    program_id INTEGER PRIMARY KEY AUTOINCREMENT,
    program_name TEXT NOT NULL
);

-- Sample Programs
INSERT INTO programs (program_name) VALUES
('BSIT'), 
('BSCS'), 
('BSCpE');

-- -------------------------
-- Table for Alumni
-- -------------------------
CREATE TABLE IF NOT EXISTS alumni (
    alumni_id INTEGER PRIMARY KEY AUTOINCREMENT,
    first_name TEXT NOT NULL,
    last_name TEXT NOT NULL,
    email TEXT UNIQUE NOT NULL,
    phone TEXT,
    graduation_year INTEGER,
    program_id INTEGER,
    FOREIGN KEY (program_id) REFERENCES programs(program_id)
);

-- Sample Alumni (5 examples)
INSERT INTO alumni (first_name, last_name, email, phone, graduation_year, program_id) VALUES
('Remalyn', 'Abad', 'remalyn.abad@email.com', '09171234567', 2024, 1),
('Stacy', 'Dela Cruz', 'stacy.delacruz@email.com', '09181234567', 2023, 2),
('Orange', 'Santos', 'orange.santos@email.com', '09201234567', 2025, 1),
